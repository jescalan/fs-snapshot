{
  "name": "fs-snapshot",
  "description": "creates a snapshot of a directory which can be used to determine which files have changed",
  "version": "0.1.0",
  "author": "Jeff Escalante",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/jescalan/fs-snapshot/issues",
  "devDependencies": {
    "ava": "0.15.x",
    "coveralls": "2.x",
    "nyc": "6.x",
    "standard": "7.x"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "homepage": "https://github.com/jescalan/fs-snapshot",
  "license": "MIT",
  "main": "lib",
  "repository": "jescalan/fs-snapshot",
  "scripts": {
    "test": "ava",
    "lint": "standard",
    "coverage": "nyc ava",
    "coveralls": "nyc --reporter=lcov ava && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "objectfn": "^2.0.0",
    "readdirp": "^2.1.0",
    "when": "^3.7.7"
